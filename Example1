//WAJP to check if given number is an even number or an odd number
class Launch
{
    public static void main(String[] args)
    {
        int num=23;
        
        if(num%2==0)
        {
            System.out.println("It is an Even number");
        }
        else
        {
            System.out.println("It is an odd number");
        }
    }
}
